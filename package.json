{
  "name": "ty-open",
  "version": "1.1.3",
  "description": "Site Favorites For Terminal",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "node ./change-pkg.js config.base ./lib && rimraf lib",
    "build": "babel src -d lib",
    "lint": "eslint -c package.json ./src --fix --no-eslintrc",
    "help": "ty-help",
    "postinstall": "make-dir config && npm run build"
  },
  "bin": {
    "to": "./bin/ty-open"
  },
  "keywords": [
    "command",
    "site",
    "url",
    "terminal",
    "favorites"
  ],
  "author": "tingyuan",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "make-dir-cli": "^1.0.0",
    "rimraf": "^2.6.1",
    "ty-help": "^1.0.4"
  },
  "dependencies": {
    "ajv": "^5.2.0",
    "chalk": "^2.0.1",
    "deepmerge": "^1.4.4",
    "minimist": "^1.2.0",
    "node-emoji": "^1.5.1",
    "opn": "^5.0.0",
    "url-regex": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/ty-open.git"
  },
  "bugs": {
    "url": "https://github.com/lovetingyuan/ty-open/issues"
  },
  "homepage": "https://github.com/lovetingyuan/ty-open#readme",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "no-plusplus": "off"
    }
  },
  "eslintIgnore": [
    "lib",
    "index.js",
    "change-pkg.js"
  ],
  "config": {
    "base": "./lib"
  }
}